num1 = float(input("Enter first number: "))
num2 = float(input("Enter second number: "))
s = input("Choose an operator (+, -, *, /): ")
if s == "+":
    result = num1 + num2
elif s == "-":
    result = num1 - num2
elif s == "*":
    result = num1 * num2
elif s == "/":
    result = num1 / num2
else:
    print("Invalid choice.")
print("Result:", result)
